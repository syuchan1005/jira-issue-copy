{
    "manifest_version": 2,
    "version": "1.0.3",
    "name": "Jira Issue Copy",
    "permissions": ["activeTab", "storage", "clipboardWrite"],
    "content_security_policy": "script-src 'self' 'sha256-7J/Bd0/flRIAULm8ehX3pYEicOEMY7PBKFB9eLh/uDQ=' https://www.google-analytics.com; object-src 'self'",
    "browser_action": {
        "default_title": "Jira Issue Copy",
        "default_popup": "popup.html"
    },
    "description": "Jira issue number and title to clipboard",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "author": "syuchan1005<syuchan.dev@gmail.com>"
}
