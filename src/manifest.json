{
  "manifest_version": 2,
  "name": "Jira Issue Copy",
  "author": "syuchan1005<syuchan.dev@gmail.com>",
  "permissions": [
    "activeTab",
    "storage",
    "clipboardWrite",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' 'sha256-ov2blWk7bkz3uoPG+i+U6oivfuDHAQb9MTmg0FHxy7E=' 'sha256-wkjjkobC2KELBhEF86F3liMYLHUGTI36CCNflnQF+3Q=' https://www.google-analytics.com; object-src 'self'",
  "background":{
    "scripts":[
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Jira Issue Copy",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
